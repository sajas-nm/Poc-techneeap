{
    "parser": "@typescript-eslint/parser",

    "env": {
        "browser": true
    },
    "extends": [],
    "parserOptions": {
        "project": ["tsconfig.json"],
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "@typescript-eslint",
        "react",
        "react-hooks",
        "eslint-plugin-import",
        "prettier"
    ],
    "rules": {},
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
